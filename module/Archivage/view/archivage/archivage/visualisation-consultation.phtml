      function nbJours($debut, $fin) {
      	//60 secondes X 60 minutes X 24 heures dans une journï¿½e
      	$nbSecondes= 60*60*24;
      
      	$debut_ts = strtotime($debut);
      	$fin_ts = strtotime($fin);
      	$diff = $fin_ts - $debut_ts;
      	return round($diff / $nbSecondes);
      }
